
@{
    ViewData["Title"] = "SearchSp";
    Layout = "~/Views/Shared/_LayoutOgani.cshtml";
}
@using X.PagedList.Mvc.Core;
@model X.PagedList.BasePagedList<Product>;

@foreach(var item in Model)
{
    @await Html.PartialAsync("PartialSanPham", item);
}
<div class="product__pagination">
    @Html.PagedListPager(Model, page =>Url.Action("SearchSP", new{page=page, search=ViewBag.TenSp}))
</div>
